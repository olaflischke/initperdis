<Window x:Class="TradingDayAnalyzerUi.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TradingDayAnalyzerUi"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Canvas>
        <TextBox Width="80" Height="23" Canvas.Left="35" Canvas.Top="10" Text="{Binding ElementName=slider, Path=Value}" />

        <Slider x:Name="slider" Width="80" Canvas.Left="10" Canvas.Top="38" />
    </Canvas>
</Window>
